{
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "max-len": "off",
    // "avoidEscape": 0,
    // "allowTemplateLiterals": 0,
    // "quotes": 0,
    "no-underscore-dangle": "off",
    "no-useless-concat": "off",
    "prefer-destructuring": "off",
    "no-useless-escape": "off",
    "no-shadow": "off",
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "next"
      }
    ]
  }
}